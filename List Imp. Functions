#include<bits/stdc++.h>
using namespace std;

void display(list<int> &l)
{
    cout<<"Size of : "<<l.size()<<endl;

    list<int>::iterator it;
    for(it = l.begin();it != l.end();++it)
    {
        cout<<(*it)<<" ";
    }
    cout<<endl;
}

